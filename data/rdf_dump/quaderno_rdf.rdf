   <rdf:Description rdf:about="http://w3id.org/bufalinis-notebook/comment/comm-084">
      <rdf:type rdf:resource="http://www.w3.org/ns/oa#Annotation"/>
      <oa:hasTarget rdf:resource="http://w3id.org/bufalinis-notebook/citation/a-manzoni-la-monaca-di-monza"/>
      <oa:hasBody>
         <rdf:Description rdf:about="http://w3id.org/bufalinis-notebook/note/comm-084">
            <rdf:value rdf:parseType="XMLLiteral">
               <section xmlns="http://www.tei-c.org/ns/1.0"
                        type="comment"
                        xml:id="comm-084"
                        corresp="#quot-127">
                  <ab type="ellipsis">...</ab>
                  <p rend="left-square-bracket right-square-bracket">Ecco una tipica impennata poetica: <lb/>di grande, tragica poesia!</p>
               </section>
            </rdf:value>
            <prism:startingPage rdf:resource="http://w3id.org/bufalinis-notebook/page/84"/>
         </rdf:Description>
      </oa:hasBody>
