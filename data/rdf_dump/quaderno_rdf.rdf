   <rdf:Description rdf:about="http://w3id.org/bufalinis-notebook/citation/t-campanella-madrigali">
      <rdf:type rdf:resource="http://purl.org/spar/cito/Citation"/>
      <cito:hasCitingEntity rdf:resource="http://w3id.org/bufalinis-notebook/page/38"/>
      <cito:hasCitedEntity>
         <rdf:Description rdf:about="http://w3id.org/bufalinis-notebook/excerpt/bibl060">
            <rdf:value rdf:parseType="XMLLiteral">
               <section xmlns="http://www.tei-c.org/ns/1.0">
                  <lg>
                     <l>Senza lutto se fosse, senza senso</l>
                     <l>sarian le cose e senza godimento,</l>
                     <l>né l'un contrario l'altro sentirebbe,</l>
                     <l>né ci saria tra lor combattimento,</l>
                     <l>né generazione, e il caos immenso</l>
                     <l>la bella distinzione assorbirebbe</l>
                  </lg>
               </section>
            </rdf:value>
            <prism:startingPage rdf:resource="http://w3id.org/bufalinis-notebook/page/38"/>
         </rdf:Description>
      </cito:hasCitedEntity>
      <cito:hasCitedEntity>
         <rdf:Description rdf:about="http://w3id.org/bufalinis-notebook/excerpt/bibl060a">
            <rdf:value rdf:parseType="XMLLiteral">
               <section xmlns="http://www.tei-c.org/ns/1.0">
                  <l>La morte è dolce a chi la vita è amara</l>
               </section>
            </rdf:value>
            <prism:startingPage rdf:resource="http://w3id.org/bufalinis-notebook/page/38"/>
         </rdf:Description>
      </cito:hasCitedEntity>
      <cito:hasCitedEntity>
         <rdf:Description rdf:about="http://w3id.org/bufalinis-notebook/excerpt/bibl060b">
            <rdf:value rdf:parseType="XMLLiteral">
               <section xmlns="http://www.tei-c.org/ns/1.0">
                  <lg>
                     <l>Questa dolente vita</l>
                     <l>peggior di mille morti.</l>
                  </lg>
               </section>
            </rdf:value>
            <prism:startingPage rdf:resource="http://w3id.org/bufalinis-notebook/page/38"/>
         </rdf:Description>
      </cito:hasCitedEntity>
      <cito:hasCitedEntity>
         <rdf:Description rdf:about="http://w3id.org/bufalinis-notebook/excerpt/bibl060c">
            <rdf:value rdf:parseType="XMLLiteral">
               <section xmlns="http://www.tei-c.org/ns/1.0">
                  <lg>
                     <l>Privata invidia ed interesse infetta</l>
                     <l>Italia mia.</l>
                  </lg>
               </section>
            </rdf:value>
            <prism:startingPage rdf:resource="http://w3id.org/bufalinis-notebook/page/38"/>
         </rdf:Description>
      </cito:hasCitedEntity>
      <cito:hasCitedEntity>
         <rdf:Description rdf:about="http://w3id.org/bufalinis-notebook/excerpt/bibl060d">
            <rdf:value rdf:parseType="XMLLiteral">
               <section xmlns="http://www.tei-c.org/ns/1.0">
                  <lg>
                     <l>E del fuoco, più soffiato, più s'accende,</l>
                     <l>poi vola in alto e di stelle s'infiora</l>
                  </lg>
               </section>
            </rdf:value>
            <prism:startingPage rdf:resource="http://w3id.org/bufalinis-notebook/page/38"/>
         </rdf:Description>
      </cito:hasCitedEntity>
      <cito:hasCitedEntity>
         <rdf:Description rdf:about="http://w3id.org/bufalinis-notebook/excerpt/bibl060e">
            <rdf:value rdf:parseType="XMLLiteral">
               <section xmlns="http://www.tei-c.org/ns/1.0">
                  <l>Snella per tutto il mondo e lieta vola.</l>
               </section>
            </rdf:value>
            <prism:startingPage rdf:resource="http://w3id.org/bufalinis-notebook/page/38"/>
         </rdf:Description>
      </cito:hasCitedEntity>
