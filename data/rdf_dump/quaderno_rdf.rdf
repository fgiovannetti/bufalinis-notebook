            <rdf:value rdf:parseType="XMLLiteral">
               <section xmlns="http://www.tei-c.org/ns/1.0">
                  <p>pravvivere.</p>
               </section>
            </rdf:value>
            <dcterms:relation rdf:resource="http://w3id.org/bufalinis-notebook/note/comm-134"/>
