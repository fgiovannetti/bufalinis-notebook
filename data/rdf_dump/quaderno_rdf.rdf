   <rdf:Description rdf:about="http://w3id.org/bufalinis-notebook/comment/tra-031">
      <rdf:type rdf:resource="http://www.w3.org/ns/oa#Annotation"/>
      <oa:hasTarget rdf:resource="http://w3id.org/bufalinis-notebook/citation/m-yourcenar-memorie-di-adriano-b"/>
