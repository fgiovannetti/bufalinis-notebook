   <rdf:Description rdf:about="http://w3id.org/bufalinis-notebook/comment/comm-082">
      <rdf:type rdf:resource="http://www.w3.org/ns/oa#Annotation"/>
      <oa:hasTarget rdf:resource="http://w3id.org/bufalinis-notebook/citation/a-manzoni-la-monaca-di-monza"/>
      <oa:hasBody>
         <rdf:Description rdf:about="http://w3id.org/bufalinis-notebook/note/comm-082">
            <rdf:value rdf:parseType="XMLLiteral">
               <section xmlns="http://www.tei-c.org/ns/1.0"
                        type="comment"
                        xml:id="comm-082"
                        corresp="#quot-123">
                  <p rend="left-square-bracket">Spesso vi è nel <persName ref="#AM">Manzoni</persName> una capacità <lb/>di penetrazione psicologica, una conoscenza <lb/>dei sentieri e delle vie che portano al vizio <lb/>e alla perdizione <del rend="overstrike">più che</del>
                     <add place="above">alquanto</add> sospetta.</p>
                  <p rend="right-square-bracket">Non diversamente <add place="above">strana e</add> sospetta appariva <lb/>a Lucia nella Signora, (in <add place="above">una</add> monaca!) <del rend="overstrike">tale</del>
                     <add place="above">una simile</add> conoscenza!</p>
               </section>
            </rdf:value>
            <dcterms:references rdf:resource="http://w3id.org/bufalinis-notebook/person/am"/>
            <prism:startingPage rdf:resource="http://w3id.org/bufalinis-notebook/page/84"/>
         </rdf:Description>
      </oa:hasBody>
      <oa:motivatedBy>
         <rdf:Description rdf:about="http://w3id.org/bufalinis-notebook/comment">
