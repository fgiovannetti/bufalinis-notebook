   <rdf:Description rdf:about="http://w3id.org/bufalinis-notebook/citation/marziale-epigrammi-5-34">
      <rdf:type rdf:resource="http://purl.org/spar/cito/Citation"/>
      <cito:hasCitingEntity rdf:resource="http://w3id.org/bufalinis-notebook/page/52"/>
      <cito:hasCitedEntity>
         <rdf:Description rdf:about="http://w3id.org/bufalinis-notebook/excerpt/bibl081">
            <rdf:value rdf:parseType="XMLLiteral">
               <section xmlns="http://www.tei-c.org/ns/1.0">
                  <lg>
                     <l>Hanc tibi, Fronto pater, genetrix Faccilla, puellam</l>
                     <l>oscula commendo deliciasque meas,</l>
                     <l>parvola ne nigras horrescat Erotion umbras</l>
                     <l>oraque Tartarei prodigiosa canis.</l>
                     <l>Impletura fuit sextae modo frigora brumae,</l>
                     <l>vixisset totidem ni minus illa dies.</l>
                     <l>Inter tam veteres ludat lasciva patronos</l>
                     <l>et nomen blaeso garriat ore meum.</l>
                     <l>Mollia non rigidus caespes tegat ossa nec illi,</l>
                     <l>terra, gravis fueris: non fuit illa tibi.</l>
                  </lg>
               </section>
            </rdf:value>
            <prism:startingPage rdf:resource="http://w3id.org/bufalinis-notebook/page/52"/>
         </rdf:Description>
      </cito:hasCitedEntity>
   </rdf:Description>
