   <rdf:Description rdf:about="http://w3id.org/bufalinis-notebook/citation/horatio-ars-poetica">
      <rdf:type rdf:resource="http://purl.org/spar/cito/Citation"/>
      <cito:hasCitingEntity rdf:resource="http://w3id.org/bufalinis-notebook/page/115"/>
      <cito:hasCitedEntity>
         <rdf:Description rdf:about="http://w3id.org/bufalinis-notebook/excerpt/bibl131">
            <rdf:value rdf:parseType="XMLLiteral">
               <section xmlns="http://www.tei-c.org/ns/1.0">
                  <l>Denique sit quodvis, simplex dumtaxat et unum</l>
               </section>
            </rdf:value>
            <prism:startingPage rdf:resource="http://w3id.org/bufalinis-notebook/page/115"/>
         </rdf:Description>
      </cito:hasCitedEntity>
      <cito:hasCitedEntity>
         <rdf:Description rdf:about="http://w3id.org/bufalinis-notebook/excerpt/bibl131a">
            <rdf:value rdf:parseType="XMLLiteral">
               <section xmlns="http://www.tei-c.org/ns/1.0">
                  <l>Brevis esse laboro, obscurus fio;</l>
                  <l>sectantem laevia, nervi deficiunt animique;</l>
                  <l>professus grandia, turget;</l>
                  <l>serpit humi tutus nimium timidusque procellae:</l>
                  <l>in vitium ducit culpae fuga, si caret arte</l>
               </section>
            </rdf:value>
            <prism:startingPage rdf:resource="http://w3id.org/bufalinis-notebook/page/115"/>
         </rdf:Description>
      </cito:hasCitedEntity>
      <cito:hasCitedEntity>
         <rdf:Description rdf:about="http://w3id.org/bufalinis-notebook/excerpt/bibl131b">
            <rdf:value rdf:parseType="XMLLiteral">
               <section xmlns="http://www.tei-c.org/ns/1.0">
                  <l>Nec facundia deseret hunc, nec lucidus ordo <ref xml:id="quot-156-1" target="#comm-103">(1)</ref>
                  </l>
                  <milestone unit="section" rend="short_hrule"/>
                  <l>Ordinis haec virtus erit et venus (aut ego fallor) <ref xml:id="quot-156-2" target="#comm-104">(2)</ref>
                  </l>
                  <l>ut iam nunc dicat iam nunc debentia dici</l>
               </section>
            </rdf:value>
            <prism:startingPage rdf:resource="http://w3id.org/bufalinis-notebook/page/115"/>
         </rdf:Description>
      </cito:hasCitedEntity>
      <cito:hasCitedEntity>
         <rdf:Description rdf:about="http://w3id.org/bufalinis-notebook/excerpt/bibl131c">
            <rdf:value rdf:parseType="XMLLiteral">
               <section xmlns="http://www.tei-c.org/ns/1.0">
                  <l>notum si callida verbum<ref xml:id="quot-156b-3" target="#comm-105">(3)</ref>
                  </l>
                  <l>reddiderit junctura novum.</l>
               </section>
            </rdf:value>
            <prism:startingPage rdf:resource="http://w3id.org/bufalinis-notebook/page/115"/>
         </rdf:Description>
      </cito:hasCitedEntity>
   </rdf:Description>
