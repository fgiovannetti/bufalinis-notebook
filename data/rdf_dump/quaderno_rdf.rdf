      <rdf:type rdf:resource="http://www.w3.org/ns/oa#Annotation"/>
      <oa:hasTarget rdf:resource="http://w3id.org/bufalinis-notebook/citation/hegel-scienza"/>
      <oa:hasBody>
         <rdf:Description rdf:about="http://w3id.org/bufalinis-notebook/note/comm-092">
