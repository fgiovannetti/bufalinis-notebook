         </rdf:Description>
      </oa:motivatedBy>
      <frbr:complementOf rdf:resource="http://w3id.org/bufalinis-notebook/quote/quot-123"/>
   </rdf:Description>
   <rdf:Description rdf:about="http://w3id.org/bufalinis-notebook/comment/comm-083">
      <rdf:type rdf:resource="http://www.w3.org/ns/oa#Annotation"/>
      <oa:hasTarget rdf:resource="http://w3id.org/bufalinis-notebook/citation/a-manzoni-la-monaca-di-monza"/>
