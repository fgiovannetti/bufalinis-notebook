   <rdf:Description rdf:about="http://w3id.org/bufalinis-notebook/citation/am-inni-sacri-il-nome-di-maria">
      <rdf:type rdf:resource="http://purl.org/spar/cito/Citation"/>
      <cito:hasCitingEntity rdf:resource="http://w3id.org/bufalinis-notebook/page/4"/>
      <cito:hasCitedEntity>
         <rdf:Description rdf:about="http://w3id.org/bufalinis-notebook/excerpt/bibl004">
            <rdf:value rdf:parseType="XMLLiteral">
               <section xmlns="http://www.tei-c.org/ns/1.0">
                  <l>in reverenti</l>
                  <l>accoglienze onorò l'inaspettata</l>
               </section>
            </rdf:value>
            <prism:startingPage rdf:resource="http://w3id.org/bufalinis-notebook/page/4"/>
         </rdf:Description>
      </cito:hasCitedEntity>
      <cito:hasCitedEntity>
         <rdf:Description rdf:about="http://w3id.org/bufalinis-notebook/excerpt/bibl004a">
            <rdf:value rdf:parseType="XMLLiteral">
               <section xmlns="http://www.tei-c.org/ns/1.0">
                  <lg>
                     <l>Noi sappiamo, o Maria, ch'Ei solo attenne</l>
                     <l>l'alta promessa che da Te s'udìa,</l>
                     <l>Ei che in cor la ti pose: a noi solenne</l>
                     <l>è il nome tuo, Maria.</l>
                  </lg>
                  <ab type="ellipsis">....</ab>
               </section>
            </rdf:value>
            <prism:startingPage rdf:resource="http://w3id.org/bufalinis-notebook/page/4"/>
         </rdf:Description>
      </cito:hasCitedEntity>
      <cito:hasCitedEntity>
         <rdf:Description rdf:about="http://w3id.org/bufalinis-notebook/excerpt/bibl004b">
            <rdf:value rdf:parseType="XMLLiteral">
               <section xmlns="http://www.tei-c.org/ns/1.0">
                  <l>Salve, beata: in quale età scortese</l>
                  <l>quel sì caro a ridir nome si tacque?</l>
                  <ab type="ellipsis">....</ab>
               </section>
            </rdf:value>
            <prism:startingPage rdf:resource="http://w3id.org/bufalinis-notebook/page/4"/>
         </rdf:Description>
      </cito:hasCitedEntity>
      <cito:hasCitedEntity>
         <rdf:Description rdf:about="http://w3id.org/bufalinis-notebook/excerpt/bibl004c">
            <rdf:value rdf:parseType="XMLLiteral">
               <section xmlns="http://www.tei-c.org/ns/1.0">
                  <l>Quai monti mai, quali acque</l>
                  <l>non l'udiro invocar? La terra antica</l>
                  <pb n="5" facs="quad005r"/>
                  <l>non porta sola i templi tuoi, ma quella</l>
                  <l>che il Genovese divinò, nutrica</l>
                  <l>i tuoi cultori anch'ella.</l>
                  <ab type="ellipsis">....</ab>
               </section>
            </rdf:value>
            <prism:startingPage rdf:resource="http://w3id.org/bufalinis-notebook/page/4"/>
         </rdf:Description>
      </cito:hasCitedEntity>
      <cito:hasCitedEntity>
         <rdf:Description rdf:about="http://w3id.org/bufalinis-notebook/excerpt/bibl004e">
            <rdf:value rdf:parseType="XMLLiteral">
               <section xmlns="http://www.tei-c.org/ns/1.0">
                  <lg>
                     <l>Te, quando sorge, e quando cade il die,</l>
                     <l>e quando il sole a mezzo corso il parte,</l>
                     <l>saluta il bronzo che le parole pie</l>
                     <l>invita ad onorarte.</l>
                  </lg>
                  <lg>
                     <l>Nelle paure della veglia bruna</l>
                     <l>te noma il fanciullino; a Te tremante</l>
                     <l>quando ingrossa ruggendo la fortuna,</l>
                     <l>ricorre il navigante.</l>
                  </lg>
                  <ab type="ellipsis">....</ab>
               </section>
            </rdf:value>
            <prism:startingPage rdf:resource="http://w3id.org/bufalinis-notebook/page/4"/>
         </rdf:Description>
      </cito:hasCitedEntity>
      <cito:hasCitedEntity>
         <rdf:Description rdf:about="http://w3id.org/bufalinis-notebook/excerpt/bibl004f">
            <rdf:value rdf:parseType="XMLLiteral">
               <section xmlns="http://www.tei-c.org/ns/1.0">
                  <l rend="ast_left_circled">
                     <hi rend="underline">Tu pur, beata, un dì provasti il pianto</hi>
                  </l>
                  <ab type="ellipsis">...</ab>
               </section>
            </rdf:value>
            <prism:startingPage rdf:resource="http://w3id.org/bufalinis-notebook/page/4"/>
         </rdf:Description>
      </cito:hasCitedEntity>
      <cito:hasCitedEntity>
         <rdf:Description rdf:about="http://w3id.org/bufalinis-notebook/excerpt/bibl004g">
            <rdf:value rdf:parseType="XMLLiteral">
               <section xmlns="http://www.tei-c.org/ns/1.0">
                  <l rend="underline">tanto piacque al Signor di porre in cima</l>
                  <l rend="underline">questa fanciulla ebrea</l>
                  <addSpan place="below" spanTo="#a002"/>
                  <lg>
                     <l>O prole d'Israello, o nell'estremo <del rend="overstrike">caduta</del>
                     </l>
                     <l>caduta, o da sì lunga ira contrita,</l>
                     <l>non è Costei che in onor tanto avemo</l>
                     <l>di vostra gente uscita?</l>
                  </lg>
                  <anchor xml:id="a002"/>
               </section>
            </rdf:value>
            <prism:startingPage rdf:resource="http://w3id.org/bufalinis-notebook/page/4"/>
         </rdf:Description>
      </cito:hasCitedEntity>
      <cito:hasCitedEntity>
         <rdf:Description rdf:about="http://w3id.org/bufalinis-notebook/excerpt/bibl004h">
            <rdf:value rdf:parseType="XMLLiteral">
               <section xmlns="http://www.tei-c.org/ns/1.0">
                  <l>O degli afflitti scampo,</l>
                  <l>
                     <ref target="#quot-007-1" rend="underline">inclita</ref> come il sol, terribil come</l>
                  <l>oste schierata in campo.</l>
               </section>
            </rdf:value>
            <prism:startingPage rdf:resource="http://w3id.org/bufalinis-notebook/page/4"/>
         </rdf:Description>
      </cito:hasCitedEntity>
   </rdf:Description>
